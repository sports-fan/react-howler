{
  "name": "react-howler",
  "version": "1.0.0",
  "description": "A React.js wrapper for howler.js (audio player)",
  "main": "lib/index.js",
  "scripts": {
    "coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha",
    "clean": "rimraf lib",
    "start": "webpack-dev-server --config webpack.config.js --watch --hot --progress --color",
    "lint": "standard",
    "build": "babel src -d lib",
    "example:react": "cd examples/react && npm install && npm run build",
    "prepublish": "npm run lint && npm run build",
    "postpublish": "npm run clean",
    "version": "auto-changelog --package; git add CHANGELOG.md",
    "test": "mocha test --compilers js:babel-core/register --recursive",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thangngoc89/react-howler.git"
  },
  "keywords": [
    "react-component",
    "react.js",
    "redux",
    "audio",
    "player",
    "howler",
    "howler.js"
  ],
  "author": "Khoa Nguyen <hi@khoanguyen.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thangngoc89/react-howler/issues"
  },
  "homepage": "https://github.com/thangngoc89/react-howler#readme",
  "dependencies": {
    "howler": "git+https://github.com/goldfire/howler.js.git#v2.0.0-beta7"
  },
  "peerDependencies": {
    "react": "^0.14.x",
    "react-dom": "^0.14.x"
  },
  "devDependencies": {
    "auto-changelog": "^0.2.2",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-istanbul": "^0.6.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "react": "^0.14.6",
    "react-addons-test-utils": "^0.14.6",
    "react-dom": "^0.14.6",
    "rimraf": "^2.5.0",
    "standard": "^5.4.1",
    "standard-loader": "^2.0.0",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  }
}
